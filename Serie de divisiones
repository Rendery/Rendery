#include <iostream>
#include <conio.h>

using namespace std;

int main()
{
	int Ricardo=0 , Millos, num=0,deno=1;
	double precio;

	cin >> Ricardo;
	cin >> Millos;

	// 1 + 1/3(Ricardo) + 1/ 5(Ricardo) +  1/ 7(Ricardo) ... ("Millos" n√∫meros...)=

	for (int i = 0; i < Millos; i++)
	{
		cout << 1;
		if (i != 0)
		{
			cout << " / " << deno;
		}
		if (i != Millos - 1)
		{
			cout << " + ";
		}
			num++;
			deno += 2;
			
	}


	_getch();
}
